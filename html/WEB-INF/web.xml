<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>html</display-name>
	<filter>
		<filter-name>AppframeFrontFilter</filter-name>
		<filter-class>com.ai.appframe2.web.filter.LoginFilter</filter-class>
	</filter>
	<filter>
		<filter-name>ExpireResponseHeaderFilter</filter-name>
		<filter-class>com.ai.appframe2.web.filter.expire.ExpireResponseHeaderFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/crossgridaction</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/business/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/baseserver</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/listdatasourcequery</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/gridturnpage</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/dbformrefresh</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/dbtreerefresh</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/VerifyCodeLoginServlet</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/dbtreenewservlet</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/ServletUpload</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AppframeFrontFilter</filter-name>
		<url-pattern>/vertifycodeservlet</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>ExpireResponseHeaderFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>com.ai.appframe2.web.BaseListenerServer</listener-class>
	</listener>
	<listener>
		<listener-class>com.asiainfo.crm.sec.sso.BaseListenerServer</listener-class>
	</listener>
	<servlet>
		<servlet-name>ChannelShutDownClient</servlet-name>
		<servlet-class>com.asiainfo.crm.sec.channel.ChannelShutDownClient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChannelShutDownClient</servlet-name>
		<url-pattern>/channlLogout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ListDataSoureQuery</servlet-name>
		<servlet-class>com.ai.appframe2.web.ListDataSoureQuery</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletIniter</servlet-name>
		<servlet-class>com.ai.appframe2.web.ServletIniter</servlet-class>
		<init-param>
			<param-name>TempPath</param-name>
			<param-value>/WEB-INF/template</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>baseserver</servlet-name>
		<servlet-class>com.ai.appframe2.web.BaseServer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>crm4sso</servlet-name>
		<servlet-class>com.asiainfo.crm.sec.sso.Crm4SSO</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>gridturnpage</servlet-name>
		<servlet-class>com.ai.appframe2.web.tag.GridTurnpage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>dbformrefresh</servlet-name>
		<servlet-class>com.ai.appframe2.web.tag.DBFormRefresh</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>dbtreerefresh</servlet-name>
		<servlet-class>com.ai.appframe2.web.tag.DBTreeRefreshServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>dbtreenewservlet</servlet-name>
		<servlet-class>com.ai.appframe2.web.tag.dbtree.DBTreeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CentralControl</servlet-name>
		<servlet-class>com.ai.appframe2.web.action.CentralControlServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>vertifycodeservlet</servlet-name>
		<servlet-class>com.ai.frame.loginmgr.webaction.VertifyCodeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VerifyCodeLoginServlet</servlet-name>
		<servlet-class>com.ai.appframe2.web.filter.VerifyCodeLoginServlet</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>sync12580</servlet-name>
		<servlet-class>com.asiainfo.crm.so.order.dsmp.t12580.CBossServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sync12580</servlet-name>
		<url-pattern>/cboss/sync12580.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>charExtServlet</servlet-name>
		<servlet-class>com.asiainfo.crm.esop.exe.http.CharExportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>charExtServlet</servlet-name>
		<url-pattern>/ext4esopRpt</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>InsxCbossAudit</servlet-name>
		<servlet-class>com.asiainfo.crm.so.common.web.InsxCbossAuditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsxCbossAudit</servlet-name>
		<url-pattern>/cboss/CbossAudit.do</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>CentralControl</servlet-name>
		<url-pattern>/business/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>baseserver</servlet-name>
		<url-pattern>/baseserver</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>crm4sso</servlet-name>
		<url-pattern>/crm4sso</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListDataSoureQuery</servlet-name>
		<url-pattern>/listdatasourcequery</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>gridturnpage</servlet-name>
		<url-pattern>/gridturnpage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>dbformrefresh</servlet-name>
		<url-pattern>/dbformrefresh</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>dbtreerefresh</servlet-name>
		<url-pattern>/dbtreerefresh</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VerifyCodeLoginServlet</servlet-name>
		<url-pattern>/VerifyCodeLoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>dbtreenewservlet</servlet-name>
		<url-pattern>/dbtreenewservlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>vertifycodeservlet</servlet-name>
		<url-pattern>/vertifycodeservlet</url-pattern>
	</servlet-mapping>
	<mime-mapping>
		<extension>doc</extension>
		<mime-type>application/msword</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>htc</extension>
		<mime-type>text/x-component</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>rar</extension>
		<mime-type>application/rar</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>pdf</extension>
		<mime-type>application/pdf</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>ppt</extension>
		<mime-type>application/vnd.ms-powerpoint</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>xls</extension>
		<mime-type>application/msexcel</mime-type>
	</mime-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>

